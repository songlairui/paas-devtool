{
  "name": "PaaS Devtools",
  "version": "0.0.1",
  "version_name": "init",
  "description": "mypaas 相关网站中，调试 API request/response payload",
  "manifest_version": 2,
  "icons": {
    "16": "icons/paas_16.png",
    "48": "icons/paas_48.png",
    "128": "icons/paas_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/paas_16.png",
      "48": "icons/paas_48.png",
      "128": "icons/paas_128.png"
    },
    "default_title": "Devtools",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["devtools.html", "devtools-background.html"],
  "devtools_page": "devtools-background.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "debugger"
  ],
  "content_scripts": []
}
