{
  "name": "lary devtools",
  "version": "0.0.1",
  "version_name": "init",
  "description": "playground.",
  "manifest_version": 2,
  "icons": {
    "16": "icons/LARY-16.png",
    "48": "icons/LARY-48.png",
    "128": "icons/LARY-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/LARY-16.png",
      "48": "icons/LARY-48.png",
      "128": "icons/LARY-128.png"
    },
    "default_title": "Devtools",
    "default_popup": "popups/not-found.html"
  },
  "web_accessible_resources": ["devtools.html", "devtools-background.html"],
  "devtools_page": "devtools-background.html",
  "background": {
    "scripts": ["build/background.js"],
    "persistent": false
  },
  "permissions": ["http://*/*", "https://*/*", "file:///*", "contextMenus"],
  "content_scripts": []
}
